<%= content_for :screen do %>
    <%= image_tag("pokeball.gif", alt: "Pokeball", class: "pokeballGif") %>
<% end %>
<%= content_for :buttons do %>
    <button class="strong-blue-button"/>
    <button class="green-large-button" />
    <button class="yellow-large-button" />
<% end %>
<%= content_for :moves do %>
    <div class="logo-container">
        <%= image_tag("pokemon_logo_v3.webp", alt: "Logo", class: "rotated-image") %>
    </div>
<% end %>
<%= render "pokedex" %>